{
  "presets": [
    [
      "@babel/preset-env",  {
        "targets": { 
          "node": "current" 
        }
      }
    ], "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "@modules": "./src/modules"
        }
      }
    ]
  ],
  "ignore": ["**/*.spec.ts"]
}
